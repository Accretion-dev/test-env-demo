{
  "name": "test-environment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "vue-cli-service serve",
    "selenium-test": "nodemon --watch run-backend.js --exec 'babel-node --inspect run-backend.js --presets=env'",
    "parser-ava": "nodemon --watch ./parser --exec 'babel-node --presets=env --inspect -- node_modules/ava/profile.js ./parser/test.js'",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "iview": "^3.3.3",
    "vue": "^2.6.6",
    "vue-keyboard-over": "^0.4.1",
    "vue-spinner": "^1.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "ava": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^2.46.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gifencoder": "^2.0.1",
    "kill-port": "^1.3.2",
    "nodemon": "^1.18.10",
    "selenium-webdriver": "^3.6.0",
    "tcp-port-used": "^1.0.1",
    "vue-cli-plugin-scss-base": "^0.1.10",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/name-property-casing": 0,
      "vue/max-attributes-per-line": 0,
      "vue/html-self-closing": 0,
      "vue/no-parsing-error": 0,
      "vue/html-indent": 0,
      "no-unused-vars": 0,
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
